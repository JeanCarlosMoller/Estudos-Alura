<script>
String nomeEmpresa = "Alura";
System.out.println(nomeEmpresa);
</script>


<html><body> 
Empresa <script> out.println(nomeEmpresa);</script> cadastrada com sucesso! 
</body> </html>


<html>
    <head>
        <title> Página JSP - Alura</title>
    </head>
    <body>
    	<%
			String nomeEmpresa = "Alura";
			System.out.println(nomeEmpresa);
		%>
        
        <h1>
            <%
                out.println("nomeEmpresa");
            %> cadastrada com sucesso!
        </h1>
    </body>
</html>